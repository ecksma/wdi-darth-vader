<h2>Home</h2>

<p> INFO HERE: </p>

<script>
	$(document).ready(function() {
		var p = $('p'); 
		$.ajax({
			type: 'get',
			dataType: 'json',
			url: '/api/json',
			success: function(data) {
				$(p).html(data.name + ': ' + data.message);
			},
			error: function(err) {
				console.log(err);
				$(p).html('ERRAWWWWRRRR'); 
			}
		});
	}); 

</script>