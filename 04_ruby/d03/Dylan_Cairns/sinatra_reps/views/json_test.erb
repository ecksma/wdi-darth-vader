<h1>ALL THE STUFF</h1>

<p> </p>

<script>
	$(document).ready(function() {
		var p = $('p'); 
		$.ajax({
			type: 'get',
			dataType: 'json',
			url: '/api/json',
			success: function(data) {
				$.each(data, function(key, value) {
					$(p).append('<li>' + key + ': ' + value + '</li>');
				}); 
			},
			error: function(err) {
				console.log(err);
				$(p).html('ERRAWWWWRRRR'); 
			}
		});
	}); 
</script> 