<h3 class="u-full-width">All Coupons Available</h3>

<table>
  <thead>
    <th>ID</th>
    <th>Title</th>
    <th class="description">Description</th>
    <th>Product</th>
    <th>Discount</th>
    <th>Expiration Date</th>
    <th>Modify</th>
  </thead>
  <tbody>
    <% @coupons.each do |coupon| %>
    <tr>
      <td><%= coupon.id %></td>
      <td><%= coupon.title %></td>
      <td><%= coupon.description %></td>
      <td><%= coupon.product %></td>
      <td><%= coupon.discount %></td>
      <td><%= coupon.exp_date %></td>
      <td><a href="/update/<%= coupon.id %>">Modify</a></td>
      <% end %>
    </tr>
  </tbody>
</table>
